<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="Autostart" value="false" />              <!--In den Autostart schreiben-->
    <add key="HideMainWindowOnStart" value="false" />  <!--Monitor beim Start ausblenden-->
    <add key="TopPosition" value="" />                 <!--Monitor Position oben-->
    <add key="LeftPosition" value="" />                <!--Monitor Position links-->
    <add key="ShowNameInMonitor" value="true"/>        <!--Benutzername im Monitor anzeigen lassen-->
    <add key="NotificationStartTime" value="08:30"/>   <!--Ab welcher Uhrzeit Benachrichtigungen angezeigt werden sollen-->
    <add key="NotificationStopTime" value="19:00"/>    <!--Bis welche Uhrzeit Benachrichtigungen angezeigt werden sollen-->
    <add key="NotificationWeekend" value="false"/>     <!--Ob am Wochenende Benachrichtigungen angezeigt werden sollen-->
  </appSettings>
</configuration>